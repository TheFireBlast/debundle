{
    "compilerOptions": {
        "incremental": true,
        "target": "es2020",
        "module": "commonjs",
        "outDir": "./dist",

        "strict": false,
        "declaration": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        // "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
}
